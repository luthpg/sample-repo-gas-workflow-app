{
  "name": "gas-workflow-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build:front": "tsc -b && vite build",
    "build:back": "rolldown -c && cpy server/*.json dist",
    "build": "rimraf dist/ && pnpm run build:front && pnpm run build:back",
    "check": "biome check --write",
    "preview": "vite preview",
    "push": "clasp push -f",
    "deploy": "clasp deploy",
    "deploy:update": "dotenv cross-var \"clasp deploy --deploymentId $GAS_DEPLOYMENT_ID",
    "gas": "gasnuki",
    "test": "vitest run",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@ciderjs/gasnuki": "^0.2.9",
    "@hookform/resolvers": "^5.2.1",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@tailwindcss/vite": "^4.1.12",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.13",
    "linkify-react": "^4.3.2",
    "linkifyjs": "^4.3.2",
    "lucide-react": "^0.539.0",
    "next-themes": "^0.4.6",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.62.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.12",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.0",
    "@google/clasp": "2.5.0",
    "@testing-library/jest-dom": "^6.7.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/google-apps-script": "^1.0.99",
    "@types/node": "^24.3.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@typescript/native-preview": "7.0.0-dev.20250816.1",
    "@vitejs/plugin-react": "^5.0.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "3.2.4",
    "cpy-cli": "^6.0.0",
    "cross-var": "^1.1.0",
    "dotenv-cli": "^10.0.0",
    "jsdom": "^26.1.0",
    "rimraf": "^6.0.1",
    "rolldown": "1.0.0-beta.32",
    "rolldown-plugin-remove-export": "^0.1.3",
    "tw-animate-css": "^1.3.7",
    "typescript": "~5.9.2",
    "vite": "^7.1.2",
    "vite-plugin-google-apps-script": "^0.4.0",
    "vite-plugin-singlefile": "^2.3.0",
    "vitest": "^3.2.4"
  },
  "packageManager": "pnpm@10.15.0"
}
